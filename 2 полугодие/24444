with open('24-241.txt') as f:
    s=f.readline()
    e=s.split('E')
    for i in e:
        if i.count('B')>1 and i.count('A')>5:
            e.index('B')
            o=e[::-1]
            o.index('B')
    print(e.index('B'), o.index('B'))




